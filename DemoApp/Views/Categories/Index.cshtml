@model List<DemoApp.Models.Category>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Categories Home Page</h2>

<table class="table">
    <tr>
        <th>Category ID</th>
        <th>Category Name</th>
    </tr>
   
        @foreach (var item in Model)
        { <tr>
            <td>@item.CategoryID</td>
            <td>@item.CategoryName</td>
           </tr>
          
        }
       
  
</table>
